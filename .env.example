# AI Provider Configuration
# Using Ollama (remote AI server)
# Ollama server running at remote location
OLLAMA_BASE_URL=http://45.93.251.180:11434
# Default model: llama3.1:8b (recommended for job extraction tasks)
# Other options: llama3.2:8b, llama3.1:70b, llama3.2:70b, mistral, etc.
OLLAMA_MODEL=llama3.1:8b

# Application Configuration
APP_HOST=0.0.0.0
APP_PORT=8000
DEBUG_MODE=True

# Excel File Path
# Path to the Excel file containing company data
EXCEL_FILE_PATH=data/Landratsamt.xlsx

# Processing Configuration
MAX_CONCURRENT_REQUESTS=5
REQUEST_TIMEOUT=30

# AI Rate Limiting
# Rate limiting is disabled for local Ollama (no delays between calls)
# AI_RATE_LIMIT_DELAY is deprecated and no longer used

# Frontend Callback Configuration
# URL where processed job results will be POSTed after completion
FRONTEND_CALLBACK_URL=http://localhost:5173/api/jobs/callback
